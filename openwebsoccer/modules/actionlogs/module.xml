<module version="OpenWebsoccer">
	<blocks>
		<block
			id="user-activities"
			template="user-activities-list"
			model="UserActivitiesModel"
			includepages="none"
			area="custom"
			weight="8"
			role="user,guest"/>
		<block
			id="user-activities-all"
			template="user-activities-box"
			model="AllUserActivitiesModel"
			includepages="none"
			area="custom"
			weight="8"
			role="user,guest"/>
	</blocks>
</module>
